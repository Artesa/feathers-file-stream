{
  "name": "@artesa/feathers-file-stream",
  "version": "0.4.0",
  "description": "feathers.js plugin for file uploads and downloads with streams",
  "author": "Artesa",
  "homepage": "https://github.com/Artesa/feathers-file-stream",
  "repository": {
    "type": "git",
    "url": "https://github.com/Artesa/feathers-file-stream"
  },
  "keywords": [
    "feathersjs",
    "feathers-plugin",
    "upload",
    "file",
    "stream",
    "s3"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "module": "./dist/index.mjs",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "engines": {
    "node": ">= 16.0.0"
  },
  "files": [
    "CHANGELOG.md",
    "LICENSE",
    "README.md",
    "src/**",
    "dist/**",
    "tsconfig.json",
    "vite.config.ts"
  ],
  "scripts": {
    "build": "shx rm -rf dist/ && vite build",
    "version": "npm run build",
    "release": "np",
    "test": "vitest run",
    "vitest": "vitest",
    "coverage": "nyc npm run test",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx"
  },
  "dependencies": {
    "@aws-sdk/abort-controller": "^3.208.0",
    "@aws-sdk/client-s3": "^3.209.0",
